Question 2

print("Enter a number: ")
read = input()
number = int(read)
print("Enter your name: ")
name = input()
for _ in range(number):
    print(name)

Question 3

num = False
while not num:
    answer = input("Write a positive interger ")
    if answer == '-1':
        num = True
        print("Not a positive interger")

Question 4

def is_prime(number):
    if number <= 1:
        return False
    for i in range(2, int(number**0.5) + 1):
        if number % i == 0:
            return False
    return True
num = (number you want to know if it is prime or not)
if is_prime(num):
    print(f"{num} is a prime number.")
else:
    print(f"{num} is not a prime number.")

